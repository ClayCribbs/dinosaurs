= simple_form_for([:admin, @photo_print_size]) do |f|
  = f.error_notification

  .form-group
    = f.input :width, input_html: { class: 'form-control' }
  .form-group
    = f.input :height, input_html: { class: 'form-control' }

  .form-actions
    = f.button :submit, class: 'btn btn-primary btn-lg'
